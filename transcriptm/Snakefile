from snakemake.utils import min_version
min_version("6.5.3")

if config["workflow"] == "pipe":
    module pipe:
        snakefile: "pipe/Snakefile"
        config: config

    use rule * from pipe as *

if config["workflow"] == "assemble":
    module assemble:
        snakefile: "assemble/Snakefile"
        config: config

    use rule * from assemble as *

else:
	pass
